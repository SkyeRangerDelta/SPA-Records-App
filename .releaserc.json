{
  "branches": [
    "master",
    {
      "name": "Experimental",
      "prerelease": "rc"
    }
  ],
  "tagFormat": "v${version}",
  "plugins": [
    [ "@semantic-release/git", {
      "assets": [
        "README.md",
        "CHANGELOG.md"
      ],
      "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }],
    [
      "@semantic-release/npm",
      {
        "npmPublish": false
      }
    ]
  ]
}