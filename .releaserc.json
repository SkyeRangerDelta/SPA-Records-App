{
  "branches": [
    "master",
    {
      "name": "Experimental",
      "prerelease": "rc"
    }
  ],
  "tagFormat": "v${version}",
  "plugins": [
    [ "@semantic-release/git", {
      "assets": [
        "package.json",
        "package-lock.json",
        "README.md",
        "CHANGELOG.md"
      ],
      "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
    }]
  ]
}